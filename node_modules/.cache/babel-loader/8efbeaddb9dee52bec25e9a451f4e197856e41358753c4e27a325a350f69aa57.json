{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useRouter } from 'vue-router';\nimport { post } from '../../utils/request.js';\nimport { reactive, toRefs } from 'vue';\nimport Toast, { useToastEffect } from '../../components/Toast';\nconst useLoginEffect = showToast => {\n  const router = useRouter();\n  const data = reactive({\n    username: '',\n    password: ''\n  });\n  const handleLogin = async () => {\n    try {\n      const result = await post('/api/user/login', {\n        username: data.username,\n        password: data.password\n      });\n      if (result?.errno === 0) {\n        localStorage.isLogin = true;\n        router.push({\n          name: 'Home'\n        });\n      } else {\n        showToast('登录失败');\n      }\n    } catch (error) {\n      showToast('请求失败');\n    }\n  };\n  const {\n    username,\n    password\n  } = toRefs(data);\n  return {\n    username,\n    password,\n    handleLogin\n  };\n};\nconst useRegisterEffect = () => {\n  const router = useRouter();\n  const handleRegisterClick = () => {\n    router.push({\n      name: 'Register'\n    });\n  };\n  return {\n    handleRegisterClick\n  };\n};\nexport default {\n  components: {\n    Toast\n  },\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: ' Login ',\n  setup() {\n    const {\n      show,\n      toastMessage,\n      showToast\n    } = useToastEffect();\n    const {\n      username,\n      password,\n      handleLogin\n    } = useLoginEffect(showToast);\n    const {\n      handleRegisterClick\n    } = useRegisterEffect();\n    return {\n      username,\n      password,\n      handleLogin,\n      handleRegisterClick,\n      show,\n      toastMessage\n    };\n  }\n};","map":{"version":3,"names":["useRouter","post","reactive","toRefs","Toast","useToastEffect","useLoginEffect","showToast","router","data","username","password","handleLogin","result","errno","localStorage","isLogin","push","name","error","useRegisterEffect","handleRegisterClick","components","setup","show","toastMessage"],"sources":["D:\\VsSpace\\vue\\jingdong\\src\\views\\login\\Login.vue"],"sourcesContent":["<template>\r\n<div class=\"wrapper\">\r\n  <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\"/>\r\n  <div class=\"wrapper__input\">\r\n    <input class=\"wrapper__input__content\" placeholder=\"请输入用户名\" v-model=\"username\">\r\n  </div>\r\n  <div class=\"wrapper__input\">\r\n    <input class=\"wrapper__input__content\" placeholder=\"请输入密码\" type=\"password\" v-model=\"password\" autocomplete=\"new-password\">\r\n  </div>\r\n  <div class=\"wrapper__login-button\" @click=\"handleLogin\">登录</div>\r\n  <div class=\"wrapper__login-link\" @click=\"handleRegisterClick\">立即注册</div>\r\n  <Toast v-if=\"show\" :message=\"toastMessage\"/>\r\n  <!-- <div class=\"wrapper__item\">\r\n    <div class=\"wrapper__item__login-link\">立即注册</div>\r\n    <div class=\"wrapper__item__separator\"></div>\r\n    <div class=\"wrapper__tags__passwrod-link\">忘记密码</div>\r\n  </div> -->\r\n</div>\r\n</template>\r\n<script>\r\nimport { useRouter } from 'vue-router'\r\nimport { post } from '../../utils/request.js'\r\nimport { reactive, toRefs } from 'vue'\r\nimport Toast, { useToastEffect } from '../../components/Toast'\r\n\r\nconst useLoginEffect = (showToast) => {\r\n  const router = useRouter()\r\n  const data = reactive({\r\n    username: '',\r\n    password: ''\r\n  })\r\n  const handleLogin = async () => {\r\n    try {\r\n      const result = await post('/api/user/login', {\r\n        username: data.username,\r\n        password: data.password\r\n      })\r\n      if (result?.errno === 0) {\r\n        localStorage.isLogin = true\r\n        router.push({ name: 'Home' })\r\n      } else {\r\n        showToast('登录失败')\r\n      }\r\n    } catch (error) {\r\n      showToast('请求失败')\r\n    }\r\n  }\r\n  const { username, password } = toRefs(data)\r\n  return { username, password, handleLogin }\r\n}\r\n\r\nconst useRegisterEffect = () => {\r\n  const router = useRouter()\r\n  const handleRegisterClick = () => {\r\n    router.push({ name: 'Register' })\r\n  }\r\n  return { handleRegisterClick }\r\n}\r\nexport default {\r\n  components: { Toast },\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: ' Login ',\r\n  setup () {\r\n    const { show, toastMessage, showToast } = useToastEffect()\r\n    const { username, password, handleLogin } = useLoginEffect(showToast)\r\n    const { handleRegisterClick } = useRegisterEffect()\r\n    return { username, password, handleLogin, handleRegisterClick, show, toastMessage }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/virables.scss';\r\n.wrapper{\r\n  position: absolute;\r\n  top:50%;\r\n  left: 0;\r\n  right: 0;\r\n  transform: translateY(-50%);\r\n  &__img{\r\n    display: block;\r\n    margin: 0 auto .4rem auto;\r\n    width: 0.66rem;\r\n    height: 0.66rem;\r\n  }\r\n  &__input{\r\n    box-sizing: border-box;\r\n    height: .48rem;\r\n    margin: 0 .4rem .16rem .4rem;\r\n    padding: 0 .16rem;\r\n    background: #F9F9F9;\r\n    border: .01rem solid rgba(0,0, 0 ,0.10);\r\n    border-radius: .06rem;\r\n    &__content{\r\n      line-height: .48rem;\r\n      border: none;\r\n      outline: none;\r\n      width: 100%;\r\n      background: none;\r\n      font-size: .16rem;\r\n      color:$content-notice-fontcolor;\r\n      &::placeholder{\r\n        color:$content-notice-fontcolor;\r\n      }\r\n    }\r\n  }\r\n  &__login-button{\r\n        margin: .32rem .4rem .16rem .4rem;\r\n        line-height: .48rem;\r\n        font-size: .16rem;\r\n        background: #0091ff;\r\n        box-shadow: 0 .04rem .08rem 0 rgba( 0, 145,255,0.32);\r\n        border-radius: .04rem;\r\n        color: #fff;\r\n        text-align: center;\r\n    }\r\n  // &__item {\r\n  //       display: flex;\r\n  //       align-items: center;\r\n  //       justify-content: center;\r\n  //       &__login-link{\r\n  //           text-align: center;\r\n  //           font-size: 0.14rem;\r\n  //           color: $content-notice-fontcolor;\r\n  //       }\r\n  //       &__passwrod-link{\r\n  //           text-align: center;\r\n  //           font-size: 0.14rem;\r\n  //           color: $content-notice-fontcolor;\r\n  //       }\r\n  //       &__separator{\r\n  //           width: 1px;\r\n  //           height: 12px;\r\n  //           background-color: #ccc;\r\n  //           margin: 0 10px;\r\n  //       }\r\n  //  }\r\n    &__login-link{\r\n        text-align: center;\r\n        font-size: .14rem;\r\n        color: $content-notice-fontcolor;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";AAoBA,SAASA,SAAQ,QAAS,YAAW;AACrC,SAASC,IAAG,QAAS,wBAAuB;AAC5C,SAASC,QAAQ,EAAEC,MAAK,QAAS,KAAI;AACrC,OAAOC,KAAK,IAAIC,cAAa,QAAS,wBAAuB;AAE7D,MAAMC,cAAa,GAAKC,SAAS,IAAK;EACpC,MAAMC,MAAK,GAAIR,SAAS,CAAC;EACzB,MAAMS,IAAG,GAAIP,QAAQ,CAAC;IACpBQ,QAAQ,EAAE,EAAE;IACZC,QAAQ,EAAE;EACZ,CAAC;EACD,MAAMC,WAAU,GAAI,MAAAA,CAAA,KAAY;IAC9B,IAAI;MACF,MAAMC,MAAK,GAAI,MAAMZ,IAAI,CAAC,iBAAiB,EAAE;QAC3CS,QAAQ,EAAED,IAAI,CAACC,QAAQ;QACvBC,QAAQ,EAAEF,IAAI,CAACE;MACjB,CAAC;MACD,IAAIE,MAAM,EAAEC,KAAI,KAAM,CAAC,EAAE;QACvBC,YAAY,CAACC,OAAM,GAAI,IAAG;QAC1BR,MAAM,CAACS,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAO,CAAC;MAC9B,OAAO;QACLX,SAAS,CAAC,MAAM;MAClB;IACF,EAAE,OAAOY,KAAK,EAAE;MACdZ,SAAS,CAAC,MAAM;IAClB;EACF;EACA,MAAM;IAAEG,QAAQ;IAAEC;EAAS,IAAIR,MAAM,CAACM,IAAI;EAC1C,OAAO;IAAEC,QAAQ;IAAEC,QAAQ;IAAEC;EAAY;AAC3C;AAEA,MAAMQ,iBAAgB,GAAIA,CAAA,KAAM;EAC9B,MAAMZ,MAAK,GAAIR,SAAS,CAAC;EACzB,MAAMqB,mBAAkB,GAAIA,CAAA,KAAM;IAChCb,MAAM,CAACS,IAAI,CAAC;MAAEC,IAAI,EAAE;IAAW,CAAC;EAClC;EACA,OAAO;IAAEG;EAAoB;AAC/B;AACA,eAAe;EACbC,UAAU,EAAE;IAAElB;EAAM,CAAC;EACrB;EACAc,IAAI,EAAE,SAAS;EACfK,KAAIA,CAAA,EAAK;IACP,MAAM;MAAEC,IAAI;MAAEC,YAAY;MAAElB;IAAU,IAAIF,cAAc,CAAC;IACzD,MAAM;MAAEK,QAAQ;MAAEC,QAAQ;MAAEC;IAAY,IAAIN,cAAc,CAACC,SAAS;IACpE,MAAM;MAAEc;IAAoB,IAAID,iBAAiB,CAAC;IAClD,OAAO;MAAEV,QAAQ;MAAEC,QAAQ;MAAEC,WAAW;MAAES,mBAAmB;MAAEG,IAAI;MAAEC;IAAa;EACpF;AACF"},"metadata":{},"sourceType